%ymm1  : concatenateMInt( extractMInt( %ymm1, 0, 128), concatenateMInt( sub_double(extractMInt( %ymm2, 192, 256), extractMInt( %ymm2, 128, 192)), sub_double(extractMInt( %ymm1, 192, 256), extractMInt( %ymm1, 128, 192))))

