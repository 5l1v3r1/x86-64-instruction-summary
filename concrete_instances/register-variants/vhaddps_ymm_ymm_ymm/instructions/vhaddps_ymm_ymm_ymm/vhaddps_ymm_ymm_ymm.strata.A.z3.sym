  (concat (concat a!1
                  (add_single ((_ extract 159 128) %ymm2)
                              ((_ extract 191 160) %ymm2)))
          (concat a!2
                  (add_single ((_ extract 31 0) %ymm2)
                              ((_ extract 63 32) %ymm2)))))

